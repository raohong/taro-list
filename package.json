{
  "name": "taro-list",
  "version": "1.0.7",
  "description": "基于 Taro 的列表刷新组件, 支持下拉刷新, 上拉加载, 虚拟列表",
  "main": "dist/index.js",
  "main:h5": "dist/h5/index.js",
  "types": "./@types/index.d.ts",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build": "cross-env TARO_BUILD_TYPE=ui taro build --ui & node ./scripts/build.js && rm dist/sitemap*.json",
    "prettier": "prettier --config .prettierrc --write \"./{src,tests}/**/*.{less,ts,tsx,js}\" \"!./{build.dist}/**\"",
    "lint-staged": "lint-staged",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "author": "raohong",
  "homepage": "https://github.com/raohong/taro-list#readme",
  "keywords": [
    "taro"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raohong/taro-list.git"
  },
  "dependencies": {
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "resize-observer-polyfill": "^1.5.1",
    "taro-list-data-manager": "^0.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "npm run prettier",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@tarojs/async-await": "2.2.6",
    "@tarojs/components": "2.2.6",
    "@tarojs/components-qa": "2.2.6",
    "@tarojs/mini-runner": "^2.2.6",
    "@tarojs/plugin-babel": "2.2.6",
    "@tarojs/plugin-csso": "2.2.6",
    "@tarojs/plugin-less": "2.2.6",
    "@tarojs/plugin-uglifyjs": "2.2.6",
    "@tarojs/router": "2.2.6",
    "@tarojs/taro": "2.2.6",
    "@tarojs/taro-alipay": "2.2.6",
    "@tarojs/taro-h5": "2.2.6",
    "@tarojs/taro-qq": "2.2.6",
    "@tarojs/taro-quickapp": "2.2.6",
    "@tarojs/taro-swan": "2.2.6",
    "@tarojs/taro-tt": "2.2.6",
    "@tarojs/taro-weapp": "2.2.6",
    "@tarojs/webpack-runner": "2.2.6",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.2.6",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.2.6",
    "lint-staged": "^9.5.0",
    "nerv-devtools": "^1.5.6",
    "nervjs": "^1.5.6",
    "prettier": "^1.19.1",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.2.6",
    "stylelint-taro-rn": "2.2.6",
    "typescript": "^3.0.1"
  }
}
